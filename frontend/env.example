# Frontend Environment Variables
# Copy this file to .env.local for local development
# Copy this file to .env.production for production builds

# Backend API Base URL
# Set to empty string to disable backend calls for faster development
VITE_API_BASE_URL=http://localhost:8085/api

# Performance optimizations
VITE_DISABLE_BACKEND_CALLS=false

# Optional: Analytics or other frontend-specific variables
# VITE_ANALYTICS_ID=your_analytics_id
# VITE_SENTRY_DSN=your_sentry_dsn
